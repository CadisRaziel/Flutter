void main() {
  
  
  //essa forma é estatica, o usuario nao interage com ela
  for(int i = 0; i < 10; i += 2){
    print("For: $i");
  }
  
  
  //essa forma o usuario ja consegue interagir 
  int j = 0;
  while(j < 10){
    print("While: $j");
    j++;
  }
  
  
  //essa forma ele vai executar o codigo print do {} mesmo que a condição for falsa, porém depois que ele ir para o while e verifca a condição ai ele ve se vai continuar ou parar
  int k = 0;
  do {
    print("Do: $k");
    k++;
  } while (k < 10);
  
}
